                                  <a  class="list-group-item">
                                    <i class="fa fa-comment fa-fw"></i> <%= stock.PName %>/<%= stock.Types %>/<%= stock.Specification %>
                                    <span class="pull-right text-muted small"><em><%= stock.Quantity %></em>
                                    </span>
